{
  "name": "react-tailwind-template",
  "version": "0.0.2",
  "description": "A template repository for creating React.js projects using Tailwind CSS.",
  "author": "Jon Rubins <jonrubins@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "compile": "tsc",
    "start": "APP_ENV=development NODE_ENV=development webpack serve --progress --config webpack.config.dev.js",
    "test:watch": "DEBUG_PRINT_LIMIT=100000 jest --bail --expand --verbose --watch"
  },
  "engines": {
    "node": "14.15.1",
    "npm": "6.14.8",
    "yarn": "1.22.10"
  },
  "browserslist": "> 1%",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --cache --ext=.js,.jsx,.ts,.tsx"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sentry/react": "5.29.2",
    "@sentry/tracing": "5.29.2",
    "classnames": "2.2.6",
    "lodash": "4.17.20",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "tailwindcss": "2.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@babel/types": "7.12.12",
    "@hot-loader/react-dom": "17.0.1",
    "@testing-library/dom": "7.29.0",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.6.0",
    "@types/classnames": "2.2.11",
    "@types/jest": "26.0.19",
    "@types/lodash": "4.14.166",
    "@types/node": "14.14.16",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/segment-analytics": "0.0.33",
    "@types/testing-library__jest-dom": "5.9.5",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "autoprefixer": "10.1.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-lodash": "3.3.4",
    "core-js": "3.8.1",
    "css-loader": "5.0.1",
    "dotenv-webpack": "6.0.0",
    "eslint": "7.16.0",
    "eslint-plugin-jest-dom": "3.6.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-require-path-exists": "1.1.9",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "mini-css-extract-plugin": "1.3.3",
    "postcss": "8.2.1",
    "postcss-loader": "4.1.0",
    "prettier": "2.2.1",
    "react-hot-loader": "4.13.0",
    "regenerator-runtime": "0.13.7",
    "style-loader": "2.0.0",
    "typescript": "4.1.3",
    "webpack": "5.11.0",
    "webpack-cli": "4.3.0",
    "webpack-dev-server": "3.11.0"
  }
}
