{
  "name": "react-tailwind-template",
  "version": "0.0.1",
  "description": "A template repository for creating React.js projects using Tailwind CSS.",
  "author": "Jon Rubins <jonrubins@gmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "yarn --production=false && tsc",
    "lint": "yarn --production=false && eslint src babel.config.js buildConfig.js jest.config.js postcss.config.js prettier.config.js webpack.config.dev.js webpack.config.prod.js --cache --ext=.js,.jsx,.ts,.tsx",
    "start": "yarn && APP_ENV=development NODE_ENV=development webpack serve --progress --config webpack.config.dev.js"
  },
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6",
    "yarn": "1.22.4"
  },
  "browserslist": "> 1%",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --cache --ext=.js,.jsx,.ts,.tsx"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-router-dom": "5.2.0",
    "tailwindcss": "1.9.5"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@babel/types": "7.12.1",
    "@hot-loader/react-dom": "17.0.0",
    "@types/classnames": "2.2.10",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.6",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "autoprefixer": "10.0.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-lodash": "3.3.4",
    "copy-webpack-plugin": "6.2.1",
    "core-js": "3.6.5",
    "css-loader": "5.0.0",
    "eslint": "7.11.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-require-path-exists": "1.1.9",
    "file-loader": "6.1.1",
    "history": "4.10.1",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "mini-css-extract-plugin": "1.1.1",
    "node-sass": "4.14.1",
    "postcss": "8.1.2",
    "postcss-loader": "4.0.4",
    "prettier": "2.1.2",
    "react-hot-loader": "4.13.0",
    "react-test-renderer": "17.0.0",
    "sass-lint": "1.13.1",
    "sass-loader": "10.0.3",
    "style-loader": "2.0.0",
    "typescript": "4.0.3",
    "webpack": "5.1.3",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.0"
  }
}
